- title t '.title', name: "#{@bestellrunde.season} #{@bestellrunde.year}"

.row-fluid
  .span3
    .well
      %h4= t '.bestellrunde'
      %dl
        %dt= heading_helper Bestellrunde, :season
        %dd= h @bestellrunde.season
        %dt= heading_helper Bestellrunde, :year
        %dd= h @bestellrunde.year



= link_to t('ui.edit'), edit_admin_bestellrunde_path(@bestellrunde), class: 'btn'
= link_to t('ui.delete'), [:admin, @bestellrunde], :data => {:confirm => t('.confirm')}, :method => :delete, class: 'btn btn-danger'
